<?xml version="1.0" encoding="UTF-8" ?>

<container
  xmlns="http://symfony.com/schema/dic/services"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
  <services>
    <defaults autowire="true" autoconfigure="true" public="false" />
    <service id="DbBundle\Db\DbConfig" />
    <service id="DbBundle\Db\DbManager" />
    <service id="db.config" alias="DbBundle\Db\DbConfig" />
    <service id="db.manager" alias="DbBundle\Db\DbManager" public="true" />
    <service id="DbBundle\DataCollector\DbDataCollector">
      <tag name="data_collector" template="@Db/Collector/db.html.twig" id="db" priority="250"/>
    </service>
  </services>
</container>
