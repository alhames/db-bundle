{
  "name": "alhames/db-bundle",
  "type": "symfony-bundle",
  "description": "Symfony AlhamesDbBundle",
  "keywords": ["mysql", "database", "bundle", "symfony"],
  "homepage": "https://github.com/alhames/db-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Logachev",
      "email": "alhames@mail.ru",
      "homepage": "https://alhames.ru/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-mysqli": "*",
    "symfony/cache-contracts": "^2.0|^3.0",
    "symfony/framework-bundle": "^5.4|^6.0",
    "psr/log": "^1.1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "symfony/yaml": "^5.4|^6.0"
  },
  "suggest": {
    "symfony/web-profiler-bundle": "To use the data collector."
  },
  "support": {
    "issues": "https://github.com/alhames/db-bundle/issues"
  },
  "autoload": {
    "psr-4": {
      "Alhames\\DbBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
