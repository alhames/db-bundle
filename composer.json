{
  "name": "alhames/db-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle for MySQL/MariaDB Database",
  "homepage": "https://github.com/alhames/db-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Logachev",
      "email": "alhames@mail.ru",
      "homepage": "https://alhames.ru/"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-mysqli": "*",
    "symfony/framework-bundle": "^3.0",
    "psr/log": "^1.0",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "support": {
    "issues": "https://github.com/alhames/db-bundle/issues"
  },
  "autoload": {
    "psr-4": {
      "DbBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DbBundle\\Tests\\": "tests/"
    }
  }
}
